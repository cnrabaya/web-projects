{
    "name": "Test Extension",
    "description": "Chrome Extension Test",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["./scripts/content.js"],
            "css": ["./css/styles.css"]
        }
    ],
    "background": {
        "service_worker": "./scripts/background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "chrome extension test"
    }
}